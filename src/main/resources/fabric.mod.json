		],
		"fabric-datagen": [
			"net.andrew.testmod.TestModDataGenerator"
		],
		"terrablender": [
			"net.andrew.testmod.init.worldgen.Biome.TerrablenderAPIInit"
		]
	},
	"mixins": [
		"test-mod.mixins.json",

